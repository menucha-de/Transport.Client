<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:shared="urn:import:havis.net.ui.shared.client.widgets"
	xmlns:local="urn:import:havis.transport.ui.client.script">
	<ui:with type="havis.net.ui.shared.resourcebundle.ResourceBundle"
		field="res" />
	<ui:with type="havis.transport.ui.res.AppResources" field="localRes" />

	<ui:style>
	.text {
		font-size: 1.5rem;
		font-weight: bold;
	}
	</ui:style>

	<g:FlowPanel ui:field="valueGroup">
		<shared:CustomSuggestBox ui:field="value" />
		<local:ScriptTextArea ui:field="scriptArea"/>
	</g:FlowPanel>
</ui:UiBinder> 